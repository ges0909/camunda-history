<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.valantic.sti.mybatis.mapper.WarrantMapper">

    <select id="selectMaxWarrantNumber" resultType="Integer">
        SELECT MAX(CAST(SUBSTRING_INDEX(BUSINESS_KEY_, '_', -1) AS UNSIGNED)) AS max_number
        FROM ACT_HI_PROCINST;
    </select>

</mapper>
